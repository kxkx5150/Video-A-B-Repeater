{
   "manifest_version": 2,
   "name": "Video A-B Repeater",
   "description": "Video A-B Repeater",
   "content_scripts": [ {
      "all_frames": true,
      "run_at": "document_end",
      "js": [ "contentscript.js" ],
      "matches": [ "<all_urls>" ]
   } ],
   "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
   },
   "version": "0.8"
}
